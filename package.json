    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "cssnext": "^1.8.4",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.4.2",
    "mocha": "^2.3.2",
    "react-addons-test-utils": "^0.14.0-rc1"
